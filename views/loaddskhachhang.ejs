<html>
  <head>
      <link rel="stylesheet" href="/style/css/bootstrap-4.3.1-dist/css/anhnen.css">
      <link rel="stylesheet" href="/style/css/bootstrap-4.3.1-dist/css/bootstrap.min.css">

  </head>
  <body>


    <div style="text-align: center;"><h1>Danh Sách Khách Hàng</h1></div>
      <table class="table table-striped table-dark">
          <thead>
            <tr>
              <th scope="col">#</th>
              <th scope="col">makh</th>
              <th scope="col">tenkh</th>
              <th scope="col">cmnd</th>
              <th scope="col">email</th>
              <th scope="col">diachi</th>
              <th scope="col">gioitinh</th>
              <th scope="col">sdt</th>
          
             
              
            </tr>
          </thead>    
          <% dsx.forEach(function(kh){ %>
                  
                    <tbody>
                            <tr>
                                <th scope="row">1</th> 
                                <td><%= kh.makh %></td>
                                <td><%= kh.tenkh %></td>
                                <td><%= kh.cmnd %></td>
                                <td><%= kh.email %></td>
                                <td><%= kh.diachi %></td>
                                <td><%= kh.gioitinh %></td>
                                <td><%= kh.sdt %></td>
                           
                              
                              
                            </tr>
                    </tbody>
            
          <% });%>

  
   </table>




  </body>
</html>










